import java.util.*;
class Example{
	public static void main(String args[]){
		Scanner input=new Scanner(System.in);
		System.out.print("Input an integer : ");
		int num=input.nextInt();
		
		//find sum of digits
		int sum=0;
		while(num!=0){
			sum+=num%10;
			num/=10;
		}
		System.out.println("Sum of digits : "+sum);
	}
}
