import java.util.*;

class Example {

    public static void lcm() {
        int num = 0, i = 1;
        do{
            num++;
            for(i = 1; i<=20; i++){
                if(!(num%i == 0))break;
            }
        }while(i<=20);
        System.out.println("number :" + num);
    }

    public static void main(String args[]) {
        lcm();
    }

}
